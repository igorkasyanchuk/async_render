[<%= link_to "Header", root_path %> - <%= ("Time.now header: #{Time.now}").to_s + " " + User.count.to_s %>, numbers: <%= Array.wrap(numbers).sum %>]

<div class="navbar bg-base-100 shadow-sm">
  <div class="flex-1">
    <a class="btn btn-ghost text-xl">Parallel Render Dummy App / <%= params[:parallel].present? ? "parallel" : "NON-parallel" %></a>
  </div>
  <div class="flex gap-2">
    <input type="text" placeholder="Search" class="input input-bordered w-24 md:w-auto" />
    <div class="dropdown dropdown-end">
      <div tabindex="0" role="button" class="btn btn-ghost btn-circle avatar">
        <div class="w-10 rounded-full">
          <img
            alt="Tailwind CSS Navbar component"
            src="https://img.daisyui.com/images/stock/photo-1534528741775-53994a69daeb.webp" />
        </div>
      </div>
      <ul
        tabindex="0"
        class="menu menu-sm dropdown-content bg-base-100 rounded-box z-1 mt-3 w-52 p-2 shadow">
        <li>
          <a class="justify-between">
            Profile
            <span class="badge">New</span>
          </a>
        </li>
        <li>
          <a class="justify-between">
            <span>Categories <%= Category.count %></span>
          </a>
          <ul class="menu menu-sm dropdown-content bg-base-100 rounded-box z-1 mt-3 w-52 p-2 shadow">
            <% Category.all.each do |category| %>
              <li><a><%= category.name %></a></li>
            <% end %>
          </ul>
        </li>
        <li><a>Settings</a></li>
        <li><a>Logout</a></li>
      </ul>
    </div>
  </div>
</div>


<% # sleep(0.001) %>
